{
  "name": "Drink Water Event Popup",
  "description": "Demonstrates usage and features of the event page by reminding user to drink water",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["cookies","tabs","activeTab", "declarativeContent", "storage","https://test.salesforce.com/"],
  "background": {
    "scripts": ["background.js"]
    
  },
  "browser_action": {
    "default_title": "Drink Water Event",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/drink_water16.png",
    "32": "icons/drink_water32.png",
    "48": "icons/drink_water48.png",
    "128": "icons/drink_water128.png"
  }
}